        +------------------+
        |   Library        |
        +------------------+
        | - name           |
        | - books: List<Book> |
        | - members: List<Member> |
        +------------------+
        | +addBook()       |
        | +removeBook()    |
        | +registerMember()|
        | +displayLibrary()|
        +------------------+
                  |
                  | aggregates
                  v
        +------------------+
        |      Book        |
        +------------------+
        | - bookId         |
        | - title          |
        | - author         |
        +------------------+
        | +getDetails()    |
        +------------------+
                  ^
                  |
        +------------------+
        |   Member         |
        +------------------+
        | - memberId       |
        | - name           |
        +------------------+
        | +borrowBook()    |
        | +returnBook()    |
        +------------------+
